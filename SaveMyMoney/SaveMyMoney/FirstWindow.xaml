<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaveMyMoney"
        xmlns:User_Controls="clr-namespace:SaveMyMoney.User_Controls" x:Class="SaveMyMoney.FirstWindow"
        mc:Ignorable="d"
        Title="FirstWindow" Height="500" MinWidth="750" MinHeight="500" Width="890.734">
    <Grid x:Name="Main_grid" Background="{DynamicResource MainGreen}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="126*"/>
            <ColumnDefinition Width="126*"/>
            <ColumnDefinition Width="104*"/>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="126*"/>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="104*"/>
            <ColumnDefinition Width="126*"/>
            <ColumnDefinition Width="126*"/>



        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="64*"/>
            <RowDefinition Height="18*"/>
            <RowDefinition Height="18*"/>
            <RowDefinition Height="327*"/>
            <RowDefinition Height="46"/>
        </Grid.RowDefinitions>
        <User_Controls:Main_center_button Grid.ColumnSpan="3" Grid.Column="3" Grid.RowSpan="3" Width="Auto" Height="Auto" Panel.ZIndex="1"/>
        <Grid x:Name="Grid_Menu_Button_1" Grid.RowSpan="2" MouseUp="Grid_Menu_Button_1_MouseUp" MouseEnter="Grid_Menu_Button_1_MouseEnter" MouseLeave="Grid_Menu_Button_1_MouseLeave" Cursor="Hand">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow_50" Opacity="0.5"/>
            </Grid.Effect>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec11" Fill="{DynamicResource SecondGreen}" RadiusY="30" RadiusX="30" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}" Grid.ColumnSpan="2"/>
            <Rectangle x:Name="Rec12" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}" Grid.ColumnSpan="2"/>
            <Label x:Name="Rec1_text" Content="{DynamicResource 1_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}" Grid.ColumnSpan="2">
                <Label.Effect>
                    <DropShadowEffect x:Name="Shadow_none" Opacity="0"/>
                </Label.Effect>
            </Label>

        </Grid>
        <Grid x:Name="Grid_Menu_Button_2" Grid.RowSpan="2" Grid.Column="1" Cursor="Hand">
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec9" Fill="{DynamicResource SecondGreen}" RadiusY="26" RadiusX="26" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}"/>
            <Rectangle x:Name="Rec10" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}"/>
            <Label x:Name="Rec2_text" Content="{DynamicResource 2_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}"/>

        </Grid>
        <Grid x:Name="Grid_Menu_Button_3" Grid.RowSpan="2" Grid.Column="2" Grid.ColumnSpan="2" Cursor="Hand">
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec1" Fill="{DynamicResource SecondGreen}" RadiusY="26" RadiusX="26" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}"/>
            <Rectangle x:Name="Rec2" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}"/>
            <Label x:Name="Rec3_text" Content="{DynamicResource 3_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}" Margin="0,0,23,0"/>

        </Grid>
        <Grid x:Name="Grid_Menu_Button_4" Grid.RowSpan="2" Grid.Column="5" Grid.ColumnSpan="2" Margin="1,0,0,0" Cursor="Hand">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec3" Fill="{DynamicResource SecondGreen}" RadiusY="30" RadiusX="30" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}"/>
            <Rectangle x:Name="Rec4" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}"/>
            <Label x:Name="Rec4_text" Content="{DynamicResource 4_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}" Margin="22,0,0,0"/>

        </Grid>
        <Grid x:Name="Grid_Menu_Button_5" Grid.RowSpan="2" Grid.Column="7" Cursor="Hand">
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec5" Fill="{DynamicResource SecondGreen}" RadiusY="26" RadiusX="26" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}"/>
            <Rectangle x:Name="Rec6" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}"/>
            <Label x:Name="Rec5_text" Content="{DynamicResource 5_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}"/>

        </Grid>
        <Grid x:Name="Grid_Menu_Button_6" Grid.RowSpan="2" Grid.Column="8" Cursor="Hand">
            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="37*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="Rec7" Fill="{DynamicResource SecondGreen}" RadiusY="26" RadiusX="26" Grid.RowSpan="2" Stroke="{DynamicResource SecondGreen}"/>
            <Rectangle x:Name="Rec8" Fill="{DynamicResource SecondGreen}" Stroke="{DynamicResource SecondGreen}"/>
            <Label x:Name="Rec6_text" Content="{DynamicResource 6_bottle}" Grid.RowSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Foreground="{DynamicResource MainGreen}"/>

        </Grid>
        <Grid x:Name="Tool_grid" Grid.Row="4" Grid.ColumnSpan="9" Background="{DynamicResource SecondGreen}">
            <StackPanel Orientation="Horizontal">
                <Grid HorizontalAlignment="Left" Margin="10,0">
                    <Label Content="{DynamicResource Settings}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18" Foreground="{DynamicResource MainGreen}" BorderBrush="{DynamicResource MainGreen}" BorderThickness="0"/>
                </Grid>
                <Grid Margin="0,0,129,1">
                    <Label Content="{DynamicResource Log_out}" Foreground="{DynamicResource MainGreen}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18" Margin="10,0"/>
                </Grid>
            </StackPanel>
        </Grid>
        <Grid x:Name="MoneyIn" Grid.ColumnSpan="9" Grid.RowSpan="4" Panel.ZIndex="-1" Margin="0,49,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
            </Grid.RowDefinitions>
        </Grid>

    </Grid>
</Window>
